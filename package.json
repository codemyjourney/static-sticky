{
  "name": "gatsby-starter-default",
  "description": "Gatsby default starter",
  "version": "1.0.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "dotenv": "^6.0.0",
    "gatsby": "^2.0.0",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-styled-components": "^3.0.0",
    "gatsby-source-contentful": "^2.0.1",
    "markdown-to-jsx": "^6.7.3",
    "prop-types": "^15.6.2",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "react-helmet": "^5.2.0",
    "react-slideshow-image": "^1.0.8",
    "semantic-ui-css": "^2.4.0",
    "semantic-ui-react": "^0.82.5",
    "slash": "^2.0.0",
    "styled-components": "^3.4.9"
  },
  "keywords": [
    "static",
    "website",
    "studyassociation",
    "sticky"
  ],
  "license": "MIT",
  "scripts": {
    "//": "Run configurations to build & watch the semantic folder",
    "build-semantic": "cd src/semantic && gulp build-css build-assets",
    "watch-semantic": "cd src/semantic && npm run build-semantic && gulp watch",
    "build": "npm run build-semantic && gatsby build",
    "develop": "npm run build-semantic && gatsby develop",
    "format": "npx prettier **/*.{js,jsx} --write",
    "test": "echo \"Write tests! -> https://gatsby.app/unit-testing\""
  },
  "devDependencies": {
    "babel-plugin-root-import": "^6.1.0",
    "babel-plugin-styled-components": "^1.8.0",
    "babel-preset-gatsby": "^0.1.6",
    "gulp": "^3.9.1",
    "husky": "^1.1.1",
    "precise-commits": "^1.0.2",
    "prettier": "^1.14.2",
    "semantic-ui": "^2.4.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/svsticky/static-sticky"
  },
  "husky": {
    "hooks": {
      "//": "Best thing i could find that worked on WSL with the Git from Windows",
      "pre-commit": "npm run format"
    }
  }
}
